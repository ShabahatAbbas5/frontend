body {
  margin: 0;
  padding: 0;
  font-family: 'Roboto', sans-serif; /* Or your preferred font */
}

/* Content Wrapper */
.content-wrapper {
  display: flex;
  flex-direction: column;
  min-height: 100vh; /* Make the wrapper fill the viewport height */
}

code {
  font-family: source-code-pro, Menlo, Monaco, Consolas, 'Courier New',
    monospace;
}
